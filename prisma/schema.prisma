datasource db {
    provider = "sqlite"
    url      = env("DATABASE_URL")
}

generator client {
    provider        = "prisma-client"
    output          = "../src/generated/prisma"
    previewFeatures = ["driverAdapters", "queryCompiler"]
}

model User {
    id    Int    @id @default(autoincrement())
    email String @unique
    name  String
}
